  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "clean": "npx rimraf hooks node_modules package-lock.json && npm i",
    "build.ionic.app": "cd apps/ionic-app && npm run build:web",
    "prepare.ionic.app": "npm run clean && npm run clean.ionic.app && npm run build.ionic.app",
    "prepare.ionic.app.ios": "npm run prepare.ionic.app && cd apps/ionic-app && npm run cap.add.ios",
    "prepare.ionic.app.android": "npm run prepare.ionic.app && cd apps/ionic-app && npm run cap.add.android",
    "open.ionic.app.ios": "cd apps/ionic-app && npm run cap.ios",
    "open.ionic.app.android": "cd apps/ionic-app && npm run cap.android",
    "sync.ionic.app": "cd apps/ionic-app && npm run cap.copy",
    "start.ionic.app": "cd apps/ionic-app && npm start",
    "clean.ionic.app": "cd apps/ionic-app && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json",
    "start.web.app": "ng serve web-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
