    "@ngrx/store": "^7.0.0",
    "@workspace/scss": "file:libs/scss",
    "reflect-metadata": "^0.1.12",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.0.0",
